
(function () {

        $.ajax({
            type: 'GET',
            headers: {"cache-control": "no-cache"},
            url: prestashop.urls.shop_domain_url,
            async: true,
            data: {
                fc: 'module',
                module: '{{ moduleName }}',
                controller: '{{ frontControllerName }}',
                action: '{{ frontControllerName }}',
                ajax: true,
            },
            success: function (data) {
                alert('success');
            },
            error: function (jqXHR, statusText, errorThrown) {
                alert('error');
            }
        });
})()
